<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mapper.CityMapper">

    <select id="getAllCity" parameterType="String" resultType="City">
        SELECT * FROM city c WHERE c.orderWrold = #{orderWrold}
    </select>

    <insert id="addCity" parameterType="City">
        INSERT INTO city(cityId,orderWrold,cityName)
        VALUE(#{cityId},#{orderWrold},#{cityName})
    </insert>
</mapper>